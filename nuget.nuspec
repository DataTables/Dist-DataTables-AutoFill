<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>datatables.net-autofill</id>
        <version>2.3.5</version>
        <description>AutoFill for DataTables</description>
        <authors>SpryMedia Ltd</authors>
        <projectUrl>http://datatables.net</projectUrl>
        <license type="expression">MIT</license>
        <tags>autofill DataTables jQuery table </tags>
        <repository type="git" url="https://github.com/DataTables/Dist-DataTables-AutoFill.git"/>
        <dependencies>
            <dependency id="jquery" version="1.7.0"/>
            <dependency id="datatables.net" version="1.10.15"/>
        </dependencies>
        <contentFiles>
            <files include="**/*" buildAction="Content" />
        </contentFiles>
    </metadata>
    <files>
        <file src="js/datatables.autofill.js" target="content\Script"/>
        <file src="js/datatables.autofill.min.js" target="content\Script"/>
        <file src="types.d.ts" target="content\Script"/>

        <file src="js/datatables.autofill.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="js/datatables.autofill.min.js" target="contentFiles\any\any\wwwroot\js"/>
    </files>
</package>
